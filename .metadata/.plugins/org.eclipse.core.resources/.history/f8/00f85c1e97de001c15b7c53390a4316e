package com.dio.br;

/**
 * Singleton "apressado"
 * 
 * @author Evely
 *
 */
public class SingletonLazyHolder {

	private static SingletonLazyHolder instancia = new SingletonLazyHolder();

	private SingletonLazyHolder() {
		super();
	}
	
	public static SingletonLazyHolder getInstancia(){
		return instancia;
	}
}
